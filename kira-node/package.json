{
  "name": "kira-node",
  "version": "1.0.0",
  "description": "A simple chat api for CASBA",
  "main": "lib/app.js",
  "author": "Josla Electric <corporate@josla.com.ng>",
  "scripts": {
    "start": "node ./lib/app.js",
    "compile": "babel -d lib/ src/",
    "run": "babel -d lib/ src/ && node lib/app"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "license": "MIT",
  "repository": "paul-pagnan/simple-chat-api",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "cors": "^2.8.1",
    "dotenv": "^5.0.1",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.3",
    "http": "0.0.0",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "request": "^2.85.0",
    "socket.io": "^1.4.8",
    "watson-developer-cloud": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "istanbul": "^0.4.3"
  }
}
